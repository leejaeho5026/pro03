package kr.go.model;

import java.util.ArrayList;

public interface NoticeDAO {
	public ArrayList<NoticeDTO> getNoticeList(); //공지사항 목록
	public NoticeDTO getNotice(int no); //공지사항 상세 보기
	public int addNotice(NoticeDTO dto); //공지사항 추가
	public int int delNotice(int no); //공지사항 삭제
	public int modifyNotice(NoticeDTO dto); //공지사항 수정

}



